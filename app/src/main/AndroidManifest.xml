<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.dell.logicalgates">

    <uses-permission android:name="android.permission.INTERNET"/>
    <uses-permission android:name="android.permission.ACCESS_WIFI_STATE"/>
    <application
        android:allowBackup="true"
        android:icon="@mipmap/infuse"
        android:logo="@mipmap/infuse"
        android:label="Gate InFuse!"
        android:roundIcon="@mipmap/infuse"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity android:name=".AND" />
        <activity android:name=".OR" />
        <activity android:name=".NOT" />
        <activity android:name=".NAND" />
        <activity android:name=".NOR" />
        <activity android:name=".EOR" />
        <activity android:name=".ENOR" />

    </application>

</manifest>